{
  "name": "@iota-wiki/plugin-tutorial",
  "exports": {
    ".": "./lib/plugin.js",
    "./page": "./lib/page.js"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "yarn build:server && yarn build:client && yarn build:copy",
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "tsc --project tsconfig.client.json",
    "build:copy": "copyfiles -u 1 src/**/*.css ./lib",
    "prepack": "yarn build"
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "@docusaurus/plugin-ideal-image": "^2.0.0-beta.18",
    "@docusaurus/theme-common": "^2.0.0-beta.18",
    "@popperjs/core": "^2.11.5",
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-collapsible": "^2.8.4",
    "react-dom": "^17.0.2",
    "react-popper": "^2.2.5",
    "react-select": "^5.3.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-beta.18",
    "@docusaurus/types": "^2.0.0-beta.18",
    "@types/copyfiles": "^2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "copyfiles": "^2.4.1",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "workspaces": [
    "test"
  ],
  "files": [
    "lib"
  ]
}
